{
  "name": "jq-parser-web",
  "version": "0.0.6",
  "license": "MIT",
  "main": "dist/jq-parser-web.cjs.js",
  "module": "dist/jq-parser-web.esm.js",
  "browser": "dist/jq-parser-web.umd.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/maxiloc/jq-parser-web"
  },
  "scripts": {
    "prepare": "yarn build",
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "test": "jest && yarn build",
    "lint": "eslint src/** tests/** --ext .js",
    "lint:fix": "yarn lint -- --fix"
  },
  "renovate": {
    "extends": [
      "config:js-lib"
    ]
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.6",
    "eslint": "^7.32.0",
    "eslint-config-algolia": "^18.0.0",
    "eslint-config-import": "^0.13.0",
    "eslint-config-prettier": "^8.3.0",
    "jest": "^27.0.6",
    "jq-web": "^0.5.1",
    "node-jq": "^2.0.0",
    "prettier": "^2.3.2",
    "rollup": "^2.56.1",
    "rollup-jest": "^1.1.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-pegjs": "^2.1.3",
    "rollup-plugin-uglify": "^6.0.4"
  },
  "jest": {
    "transform": {
      "\\.pegjs$": [
        "./src/pegjs-jest.js"
      ],
      "\\.js$": [
        "rollup-jest"
      ]
    }
  }
}
